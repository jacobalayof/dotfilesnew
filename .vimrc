"set background=light
"call plug#begin('~/.vim/plugged')
"Plug 'jalvesaq/Nvim-R'
"call plug#end()
"filetype plugin indent on
"syntax on
"set number
set ignorecase
set ttyfast
inoremap jk <ESC>
"set incsearch
set hlsearch
set tabstop=4
set autoindent
set wrap linebreak nolist
set spell spelllang=en_us
:set nospell
:set backspace=indent,eol,start
